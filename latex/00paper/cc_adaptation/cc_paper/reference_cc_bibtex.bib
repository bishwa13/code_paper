	number = {4},
	urldate = {2025-01-26},
	journal = {International Journal of Applied Sciences and Biotechnology},
	author = {Dhakal, S.C. and Regmi, P.P. and Thapa, R.B. and Sah, S.K. and Khatri-Chhetri, D.B.},
